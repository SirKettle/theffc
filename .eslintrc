{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "plugins": ["react"],
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "spread": true,
      "experimentalObjectRestSpread": true
    }
  },
  "env": {
    "browser": true,
    "amd": true,
    "es6": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "arrow-parens": 0,
    "comma-dangle": ["error", "only-multiline"],
    "camelcase": "off",
    "quotes": [0, "single"],
    "no-undef": 1,
    "global-strict": 0,
    "max-len": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 0,
    "no-unused-vars": 1,
    "no-use-before-define": 0,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-confusing-arrow": 0,
    "no-mixed-operators": 0,
    "no-alert": 0,
    "react/jsx-uses-react": 1,
    "react/jsx-uses-vars": 1,
    "no-multi-spaces": ["error", { "exceptions": { "ImportDeclaration": true } }],
    "react/require-extension": 0,
    "arrow-body-style": 0,
    "no-debugger": 0,
    "react/prop-types": 0
  }
}
