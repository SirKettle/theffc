{
  "name": "holo",
  "version": "0.0.1",
  "description": "Website - Personal petcare for Gedling, Carlton and surrounding areas.",
  "main": "index.jsx",
  "repository": {
    "type": "git",
    "url": "https://github.com/SirKettle/holo"
  },
  "author": "SirKettle",
  "license": "MIT",
  "keywords": [
    "ES6",
    "babel",
    "boilerplate",
    "express",
    "hot reloading",
    "react",
    "react-hot-reloader",
    "starter",
    "webpack"
  ],
  "scripts": {
    "reset": "rimraf dist/* && rimraf node_modules/* && yarn",
    "clean": "rimraf dist/*",
    "lint": "eslint --ext .js --ext .jsx ./src",
    "start": "cross-env NODE_ENV=development webpack-dev-server --open",
    "build-dev": "yarn run clean && cross-env NODE_ENV=development webpack",
    "build-prod": "yarn run clean && cross-env NODE_ENV=production webpack",
    "test": "jest --watch",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
